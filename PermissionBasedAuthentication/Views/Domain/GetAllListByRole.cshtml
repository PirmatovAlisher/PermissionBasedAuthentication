@model List<DomainListVM>
@{
    ViewData["Title"] = "GetAllListByRole";
}

<h1>Roles Domain</h1>

<br>
<table class="table table-hover">
	<thead>
		<tr>
			<th scope="col">ID</th>
			<th scope="col">Controller Name</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model)
		{
			<tr>
				<td>@item.Id</td>
				<td>@item.ControllerName.Name</td>
			</tr>
		}
	</tbody>
</table>